[rules]
.PHONY: cleartarget all

cleartarget:
        -$(DEL) $(COMPILER_UNITTARGETDIR)/lazcontrols$(PPUEXT)

all: cleartarget $(COMPILER_UNITTARGETDIR) lazcontrols$(PPUEXT)
